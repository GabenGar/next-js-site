{
	"folders": [
		{
			"path": "."
		}
	],
	"settings": {
		"files.exclude": {
			"**/.git": true,
			"**/.svn": true,
			"**/.hg": true,
			"**/CVS": true,
			"**/.DS_Store": true,
			"**/Thumbs.db": true,
			"node_modules": true
		},
		"editor.tabSize": 2,
		"editor.rulers": [
			{
				"color": "orange",
				"column": 65
			}
		],
		"editor.wordWrap": "wordWrapColumn",
		"editor.wordWrapColumn": 65,
		"[markdown]": {
			"editor.wordWrap": "wordWrapColumn",
			"editor.quickSuggestions": false
		}
	},
	"extensions": {
		"recommendations": [
			"eamodio.gitlens",
			"webben.browserslist",
			"mikestead.dotenv"
		]
	},
}
